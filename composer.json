{
  "name": "zurean/framework",
  "description": "Zurean http-framework",
  "require": {
    "php": "^7.4",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Framework\\": "src/Framework"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
